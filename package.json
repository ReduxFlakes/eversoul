{
  "name": "surfscape",
  "version": "1.0.0rc1",
  "codename": "Liberty",
  "description": "Your friendly Neocities web portal!",
  "main": "index.js",
  "scripts": {
    "start": "npx eleventy --serve",
    "build": "npx eleventy",
    "minify-css": "cleancss -o src/css/global.min.css src/css/global.css && cleancss -o src/css/fonts.min.css src/css/fonts.css",
