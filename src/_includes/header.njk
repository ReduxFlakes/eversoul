<header class="site-header">
  <a class="site-header__title" href="/">
    <img src="/static/images/branding/petal_2x.png" alt="SurfScape Petal logo in dark gray" height="52px"></a>
  <nav class="site-navbar">
    {% for nav in nav.mainNav %}
      <a class="site-navbar__link sk-link" 
      href="{{nav.url}}" 
   {% if page.url == nav.url %}aria-current="page"{% endif %}>
        {{nav.title}}
      </a>
    {% endfor %}
  </nav>
</header>